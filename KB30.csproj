    <UseWPF>true</UseWPF>
    <ApplicationIcon>Resources\Archigraphs-Oldies-Old-House.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Add_16x.png" />
    <None Remove="Resources\PlayVideo_16x.png" />
